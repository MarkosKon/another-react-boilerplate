{
  "name": "another-react-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for React.",
  "author": "Markos Konstantopoulos",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --config webpack/webpack.dev.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ext js,jsx src",
    "lint:fix": "eslint --ext js,jsx src --fix",
    "format": "prettier-eslint --write \"src/**/*.{js,jsx}\"",
    "serve": "serve -s dist",
    "live": "live-server dist",
    "deploy": "netlify deploy --dir=dist",
    "deploy:prod": "netlify deploy --prod --dir=dist"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pre-commit": [
    "format",
    "lint"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.10.0",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "pre-commit": "^1.2.2",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "static-site-generator-webpack-plugin": "^3.4.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5",
    "workbox-webpack-plugin": "^3.6.3"
  }
}
